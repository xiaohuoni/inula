{
  "name": "inula",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "bootstrap": "inula-scripts bootstrap",
    "build": "inula-scripts turbo --cmd build",
    "cnpm": "pnpm i --registry=https://registry.npmmirror.com",
    "dep:update": "pnpm up --interactive --latest --recursive",
    "dev": "inula-scripts turbo --cmd dev --parallel",
    "format": "prettier --write .",
    "format:docs": "prettier 'docs/**/*.{md,mdx}' --write --ignore-path .gitignore --ignore-unknown",
    "format:plugin-docs": "prettier 'packages/plugin-docs/**/*' --write --ignore-unknown",
    "prepare": "husky install",
    "release": "inula-scripts release",
    "setup:webstorm": "inula-scripts setupWebStorm",
    "synccnpm": "synccnpm",
    "tsc:check": "tsc --noEmit",
    "turbo:clean": "rimraf .turbo"
  },
  "lint-staged": {
    "*.{jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@manypkg/get-packages": "^1.1.3",
    "@pnpm/lockfile-file": "^5.0.3",
    "@pnpm/logger": "^4.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@types/qrcode-terminal": "*",
    "@types/resolve": "^1.20.2",
    "@types/rimraf": "3.0.2",
    "@umijs/utils": "4.0.78",
    "@vercel/ncc": "0.33.3",
    "esno": "^0.14.1",
    "execa": "4.0.3",
    "git-repo-info": "^2.1.1",
    "husky": "^7.0.4",
    "inula-scripts": "workspace:*",
    "lint-staged": "^12.3.7",
    "matcher": "^5.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prettier-plugin-packagejson": "^2.2.17",
    "resolve": "^1.22.0",
    "rimraf": "^3.0.2",
    "synccnpm": "^1.1.3",
    "ts-node": "^10.7.0",
    "turbo": "^1.3.1",
    "typescript": "^4.7.2",
    "uglify-js": "^3.15.4",
    "yorkie": "^2.0.0",
    "zx": "^7.2.0"
  },
  "packageManager": "pnpm@7.3.0",
  "engines": {
    "node": ">=14",
    "pnpm": ">=6.20.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "esno scripts/verifyCommit.ts"
  }
}
